:@template"¦,/**
 * Main (main.scss)
 */

// Load the base
@import "_base";
@import "compass/css3/border-radius";

// If you are doing a lot of stylesheet concatenation, it is suggested
// that you scope your blueprint styles, so that you can better control
// what pages use blueprint when stylesheets are concatenated together.
body.bp {
  @include blueprint-typography(true);
  @include blueprint-utilities;
  @include blueprint-debug;
  @include blueprint-interaction;
}

form.bp {
  @include blueprint-form;
  }


/**
 * Variables
 *****/
$margin: $blueprint-grid-margin;

/**
 * Body
 ************************************************/
// Contains everything
html, body{
	height:100%;
}

body {
	font-size:12px;
	line-height: 14pt;
	font-style:normal;
	font-family: $typography-family-primary;
	color:$color-primary;
	background-image: url('../../img/bg.gif');
	background-repeat:repeat-x;
	background-color: #b2b2b2;		
}


	#wrapper {
		width: $blueprint-container-size + 2 * $blueprint-grid-margin;
		margin: 0 auto;
		overflow: hidden;		
		background-color: #fff;	
		min-height:100%;
	}
	
	// Wrapper for everything inside of the body
	#body-container {
			@include container;
	}

/*
	// inner container with margin
	#inner-container {
		margin: 10px;	
	}
*/

/**
 * Elements
 ************************************************/

// Hyperlinks 
a {
	text-decoration: none;
	outline:none;
	color: $link-color;
}
a:visited {}
a:hover {
	color: $link-color;
	text-decoration: underline;
}
a:active {}


h1, h2, h3, h4, h5, h6 {
	color: #1F1E1D;
	padding-bottom: .4em;
}

// Headers 
h1 {
	font-size: 35px;
	line-height: normal;
	font-weight: normal;
	font-family: Georgia;
	color: black;
	margin-bottom: 10px;
	margin-top: 10px;
}

h2{
	font-size:23px;
	font-weight:normal;
	font-family:Georgia;
	color:#000000;
}

h3 {
	font-size:18px;
	font-weight:normal;
	font-family:Georgia;
	color:#000000;
}
	
h4 {}
h5 {}
h6 {}

// Text elements 
strong {}
em {}
cite {}
abbr {}
acronym {}
sup {}
sub {}
address {}
ins {}
dfn {}
del {}

// Paragraphs 
p {}

// Unordered lists 
ul {}
	ul li {}
	
/* ul */
ul.nodots {
	padding:0;
	margin:0;
	list-style-type:none;
}

ul {
	list-style-position: outside;
	//padding:0 0 0 3em;
	margin:0 0 10px 0;
}

#maincontent ul li { 
	vertical-align: top;
	padding-bottom: 8px;
	line-height: 12pt;
	list-style-image: url(/Frontend/images/dlvs-12.png);
}	

// Ordered lists 
ol {}
	ol li {}

// Defition lists 
dl {}
	dt {}
	dd {}

// Blockquotes 
blockquote {}
	blockquote blockquote {}
	blockquote em {}

// Object 
object {}

// Code 
code {}
pre {}
pre code {}

// Tables 
table {}
	caption {}
	tr {}
	th {}
	td {}

// Forms 
form {}
	textarea {}
	input {}
	label {}
	option {}

// Notes 
.note {}

// Warnings/Alerts
.warning, .alert {}

// Errors
.error {}

// Downloads
.download {}

// Alignment
.left, .alignleft {}
.right, .alignright {}
.center, .aligncenter {}
.block, .alignnone {}
.clear {}

/**
 * Images
 ************************************************/

// Normal images
img {}
a img {}

// WP Smilies
img.wp-smiley {}

// Thumbnails
.thumbnail {}

// WP Captions [caption]
.wp-caption {}
	.wp-caption img {}
	.wp-caption p {}

// Gallery [gallery]
.gallery {}
	.gallery img {}

// Avatars
.avatar {}

/**
 * Header
 ************************************************/

// Contains entire header
#header-container {}

	// Contains site title and tagline
	#header {
		@include column($blueprint-grid-columns, true); 
		height:120px;
		margin-top: $blueprint-grid-margin;
	}
	
		#logo{
			@include column($blueprint-grid-columns / 2); 
		}
		
		#tools{			
			@include column($blueprint-grid-columns / 2, true); 
			text-align:right;
			color: #7A7A7A;
			font-size: 11px;			
		}		

		// Site title
		#site-title {}
			#site-title a {}
				#site-title a span {}

		// Site description/tagline
		#site-description {}

/**
 * Primary Menu
 ************************************************/

// Wrapper for entire menu area
#primary-menu {}

	// Wrapper for unordered list of menu items
	#primary-menu {
		float:left;
		width:100%;
		height:50px;
		line-height:50px;	 		
 		background: url('../../img/menubg.gif') no-repeat left center;  
 		@include border-radius(10px);
	}

		// Menu list.
		#primary-menu ul {
				list-style-type:none;				
		}
			#primary-menu li {
				float:left;
				
			}
				#primary-menu li a {   	
					display:block;			
					padding-left: 11px;	
					padding-right: 11px;
					color:#ffffff;
					text-decoration:none;
					background: url('../../img/menu_off.gif') no-repeat right top;					
				}
				
				#primary-menu li:first-child a {   	
					border-radius: 10px 0 0 10px ;
				}
				
				#primary-menu li a:hover {   
					background:url('../../img/menu_on.gif') repeat-x;

				}
				
				#primary-menu li.current-menu-item a,
				#primary-menu li.current-page-ancestor a,
				#primary-menu li.current-menu-parent a,
				#primary-menu li.current-post-parent a
				{
					background:url('../../img/menu_on.gif') repeat-x;
				}
				
				// search form
				#primary-menu #searchform{			
					position: relative;
					top: -11px;					
					margin-right: $margin / 2;					
					float: right;
				}
				
				// search field
				#primary-menu #searchform .field{				
					vertical-align: middle;
				}
				
				// search button
				#primary-menu #searchform .submit{
					vertical-align: middle;
					background: url('../../img/search_button.gif') no-repeat top center;
					border : none;
					color : transparent;
					font-size : 0;
					cursor: pointer;
					height: 22px;
					width: 46px;				
				}
			

/** 
 * Secondary Menu					
 **********/
 
	#sidebar {
		$sidebar-columns: floor($blueprint-grid-columns / 4);
		@include column($sidebar-columns); 
		font-size:11px;
	}	 
	
	#sidebar ul {
		list-style-type:none;
	}
	#sidebar ul li {
		display:inline;
		height:30px;
		line-height:30px;

	}
	#sidebar ul li a {
		color:#000000;
		float:left;
		text-decoration:none;
		width:100%;
		background:url('../../img/submenu_off.gif') repeat-x;
		text-indent: 30px;
	}
	#sidebar ul li a:hover,
	#sidebar ul .current-menu-item a {
		background:url('../../img/submenu_on.gif') repeat-x;
		color:#000000;
		text-decoration:underline;
		font-weight: normal;
	}
/**
 * Containers below the header and navigation
 ************************************************/

// Wrapper for the content and primary/secondary widget sections
#container {}

	/**
	 * Content
	 ************************************************/	

	// Content for pages with sidebar
	#content {
	    $content-columns: ceil(3 * $blueprint-grid-columns / 4);
		@include column($content-columns, true);
	}
	
	// Content without sitebar
	body.page-template-no-sidebar-php #content {
		@include column($blueprint-grid-columns); 
	}	

		// Breadcrumb trail
		.breadcrumb {}
			.breadcrumb-trail {}

		// Archive and search <div> before posts are listed
		.archive-info
		.author-info {}
		.term-info
		.category-info {}
		.tag-info {}
		.date-info {}
		.search-info {}

			// Archive and search page titles.
			.archive-title
			.author-title {}
			.term-title {}
			.category-title {}
			.tag-title {}
			.date-title {}
			.search-title {}

		// Entry wrapper <div>
		.hentry {}
		.post { margin-bottom: $margin;}
		.page {}

			// Post, page, and 404 page titles
			.entry-title {}
			.post-title {}
			.page-title {}
			.attachment-title {}
			.error-404-title {}

			// Author and date byline
			.byline {}

			// Full entries
			.entry-content {}

			// Excerpts (introduced)
			.entry-summary {}

				// Using <!--nextpage-->
				p.page-links a {}

				// More link <!--more-->
				a.more-link {}

			// Entry metadata
			.entry-meta {}

		// Post series
		.series {} 
			.series-title {}

		// Navigation links
		.navigation-links {}
			.navigation-links .previous {}
			.navigation-links .next {}

		// WP PageNavi (plugin)
		.wp-pagenavi {}
			.wp-pagenavi .pages {}
			.wp-pagenavi .current {}
			.wp-pagenavi .extend {}
			.wp-pagenavi a {}

		// Attachment image navigation
		.navigation-attachment {}

		/**
		 * Comments template
		 ************************************************/

		// Wrapper for entire comments area
		#comments-template {}

			// Comments section headers/titles
			.comments-header {}
			#comments-number {}

			// Comments closed paragraph
			p.comments-closed {}

				// Main comments section
				#comments {}
					#comments ol {}

						// Individual comments (note that WordPress has even/odd backwards)
						#comments li {}
							#comments li li {}
								#comments li li li {}
									#comments li li li li {}
						#comments li.even {}
						#comments li.odd {}
						#comments li.thread-even {}
						#comments li.thread-odd {}
						#comments li.author {}
						#comments li.trackback {}
						#comments li.pingback {}

							// Comment avatars
							#comments .avatar {}

							// Comment metadata
							#comments .comment-meta {}
								#comments .comment-meta cite {}

							// Comment text
							#comments .comment-text {}

					// Paged comments navigation
					#comments-template .paged-navigation {}
						#comments-template .paged-navigation .page-numbers {}
						#comments-template .paged-navigation .next {}
						#comments-template .paged-navigation .prev {}

				// Main comment form div
				#respond {}

					// Comment form title
					#reply {}

					// Comment form elements
					p#cancel-comment-reply {}
					#respond form {}
						#respond .form-author {}
							#respond input {}
						#respond .form-email {}
						#respond .form-url {}
						#respond .form-textarea {}
							#respond textarea {}
						#respond #submit {}
							#respond #submit:hover {}

		/**
		 * Utility widget sections (see other widget code below)
		 ************************************************/

		// All utility widget sections (except widgets template)
		.utility {}

		// Before content
		#utility-before-content {}

		// After content
		#utility-after-content {}

		// After post but before comments
		#utility-after-single {}

		// After page but before comments
		#utility-after-page {}

		// Widgets page template widgets
		.page-template-widgets #content .widget {}

	/**
	 * Widget sections (Primary and Secondary)
	 ************************************************/

	// Widgets in general
	.widget {}
		.widget-inside {}
			.widget-title {}

	// Primary widget section
	#primary {}
		#primary .widget {}
			#primary .widget-inside {}
				#primary .widget-title {}
				#primary .widget ul {}
					#primary .widget ul li {}

	// Secondary widget section
	#secondary {}
		#secondary .widget {}
			#secondary .widget-inside {}
				#secondary .widget-title {}
				#secondary .widget ul {}
					#secondary .widget ul li {}

/**
 * Footer
 ************************************************/

// Wrapper for the entire footer
#footer-container {}

	// Subsidiary widget section
	#subsidiary {}
		#subsidiary .widget {}
			#subsidiary .widget-inside {}
				#subsidiary .widget-title {}

	// Footer section
	#footer {
		@include column($blueprint-grid-columns, true); 	
	}

		#footer .text {	
			@include column(7 * $blueprint-grid-columns / 8); 			
			margin-top: $margin;
			padding-top: $margin / 2;
			border-top: solid 1px #EEEDED;
			color: #898887;
			font-size: 11px;
		}
		
		#footer .logo {	
			@include column(1 * $blueprint-grid-columns / 8, true); 	
			text-align:right;				
		}
		
		

		// Footer extra
		#footer .copyright {}
		#footer .credit {}
