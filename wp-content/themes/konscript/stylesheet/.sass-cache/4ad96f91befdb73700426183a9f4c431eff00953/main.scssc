/**
 * Header
 ************************************************/

// Contains entire header
#header-container {}

	// Contains site title and tagline
	#header {
		@include column($body-container-cols, true); 
		height:120px;
		margin: $blueprint-grid-margin 0;
	}
	
		#logo{
			@include column($body-container-cols / 2); 
			height: 67px;
		}
		
		#tools{			
			@include column($body-container-cols / 2, true); 
			text-align:right;
			font-size: 11px;			
