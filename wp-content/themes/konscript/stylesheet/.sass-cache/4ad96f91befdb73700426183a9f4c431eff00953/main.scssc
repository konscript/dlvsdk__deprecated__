/**
 * wysiwyg styles
 ********************************/
img.alignright {float:right; margin:0 0 1em 1em}
img.alignleft {float:left; margin:0 1em 1em 0}
img.aligncenter {display: block; margin-left: auto; margin-right: auto}
.alignright {float:right; }
.alignleft {float:left; }
.aligncenter {display: block; margin-left: auto; margin-right: auto}  


/**
 * frontpage content slider 
 */
 #featured { 
 
 	// variables
	$featured-height: 182px;
	$tabs-cols: $body-container-cols * 1 / 3;
	$content-cols: $body-container-cols - $tabs-cols;
 
	@include column($body-container-cols, true); 
	position:relative; 
	height:$featured-height; 
	margin-bottom: $blueprint-grid-margin;
	border: 1px solid #ccc;

	/* all tabs container */
	ul.ui-tabs-nav{ 
		list-style:none;	 
		@include column($tabs-cols + 1, true); 
	}

	/* single tab container  */
	ul.ui-tabs-nav li{ 
		@include linear-gradient(color-stops(#f9f9f9, #eee));
		border-left: 1px solid #ccc;
		border-bottom: 1px solid #ccc;		
		margin: 0;
		text-indent: 0px;	
		//background-image: url(../../img/content-slider-inactive-icon.png);
		//background-position: 95% 50%;		
	}

	/* single tab link */
	ul.ui-tabs-nav li.ui-tabs-nav-item a{ 
		padding: 5px 0;
		display:block;
		text-decoration: none;
		height:50px; 
		color:#000;  
		//margin-left: 20px;		
		font-size: 16px;		
	}

	// image
	ul.ui-tabs-nav li.ui-tabs-nav-item a img{	 
		display: table-cell;
		vertical-align: middle;
		@include prepend(1);				
		@include column($tabs-cols / 5); 		
		margin-top: 5px;
	}

	// text
	ul.ui-tabs-nav li.ui-tabs-nav-item a div{ 
		line-height: 40px;
		@include column(($tabs-cols * 4  / 5) - 1, true); 		
		margin-top: 5px;
	}
	/* selected/hovered tab */
	ul.ui-tabs-nav li.ui-tabs-nav-item:hover,
	ul.ui-tabs-nav li.ui-tabs-selected{
		background-image: url(../../img/content-slider-active-icon.png);
		background-position: 95% 50%;
		background-repeat: no-repeat;		
	}



	/* slider content */
	.ui-tabs-panel{ 
		@include column($content-cols, true); 		
		position: relative;
		overflow:hidden;
		
		img {
			position:absolute;
			top:0;
			left: 0;
			z-index: 1;
		}
		
		.left {
			border-right: 1px solid #ccc;
			width: ($content-cols / 2 * $blueprint-column-width) - 8;
			height:  $featured-height;			
			float:left;	
			position:relative;
			z-index: 2;	
		}
		
		.right {
			position:relative;
			z-index: 2;	
			width: $content-cols / 2 * $blueprint-column-width;
			float:left;
			height:  $featured-height;					
			/* Fall-back for browsers that don't support rgba */	
			background: rgb(255,255,255); 
			filter:progid:DXImageTransform.Microsoft.gradient(startColorstr=#99ffffff, endColorstr=#99ffffff);
			-ms-filter: "progid:DXImageTransform.Microsoft.gradient(startColorstr=#99ffffff, endColorstr=#99ffffff)";
			background: rgba(255,255,255, .6);
			
			div.inner {
				margin: $blueprint-grid-margin;			
			}
		}
		
	}
	.ui-tabs-panel.ui-tabs-hide{ display:none; }
	
}
