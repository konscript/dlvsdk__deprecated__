/**
 * Primary Menu
 ************************************************/
	
	// Wrapper for entire menu area
	#primary-menu {
		overflow:hidden;	
		float:left;
		@include column($body-container-cols, true); 
		height:50px;
 		background: url('../../img/menubg.gif') repeat-x left center;  
 		@include border-radius($radius);
	}

		// Menu list.
		$searchform-cols: 9;
		#primary-menu ul {
				list-style: none;
				@include column($body-container-cols - $searchform-cols); 
		}
			#primary-menu li {
				float:left;
				line-height:50px;				
			}
				#primary-menu li a {   	
					display:block;			
					padding-left: 11px;	
					padding-right: 11px;
					color:#ffffff;
					text-decoration:none;
					border-right: 2px groove #BBB;
				}
				
				// rounded corners on very first menu item
				#primary-menu li:first-child a {   	
			 		@include border-radius($radius 0 0 $radius);					
				}

				// selected item AND hovered item 
				#primary-menu li a:hover,			
				#primary-menu li.current-menu-item a,
				#primary-menu li.current-menu-parent a,
				#primary-menu li.current-post-parent a,
				#primary-menu li.current_page_parent a,
				#primary-menu li.current-page-ancestor a				
				{					
					background:url('../../img/menu_on.gif') repeat-x;
					border-right: 2px solid #bc2127;
			
				}
				
				// search form
				#primary-menu #searchform{	
					@include column($searchform-cols, true);					
					text-align:right;
					height:22px;
					position:relative;
					margin-top:12px;
					 											
				}
				
				// search field
				#primary-menu #searchform .field{				
					width: 80px;
					border: 0;
					padding: 2px;
			 		@include border-radius(2px);	
					vertical-align: middle;	 						
				}
				
				// search button
				#primary-menu #searchform .submit{
					background: url('../../img/search_button.gif') no-repeat top center;
					border : none;
					color : transparent;
					font-size : 0;
					cursor: pointer;
					height: 22px;
					width: 46px;	
					vertical-align: middle;	
				}
			

